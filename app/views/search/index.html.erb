<h1> Stations </h1>

<% @station_data.get_stations.each do |station| %>
  <ul class = 'station'>
    <li> <%= station.name %> </li>
    <li> <%= station.address %> </li>
    <li> <%= station.fuel_type %> </li>
    <li> <%= station.distance %> </li>
    <li> <%= station.access_times %> </li>
  </ul>
<% end %>
